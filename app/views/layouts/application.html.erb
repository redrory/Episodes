<!DOCTYPE html>
<html>
<head>
  <title>Episodes</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

	<% flash.each do |name,message| %>
	<%=content_tag :div, message, :id => "flash_#{name}"%>
	<% end %>
<%= yield %>
</body>
</html>
